pipeline{
    agent any
        stages
        {
            stage("Npm Install"){
            steps{
                bat 'npm install'
            }
             }
            stage("run app"){
                steps{
            
                 bat 'npm run start'
            
                }
            }
             stage('Execute tests') {
                    steps {
                        bat 'npm test'
          }
             }
        }
    }
