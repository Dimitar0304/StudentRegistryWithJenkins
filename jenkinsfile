pipeline{
    agent any
        stages
        {
            stage("Checkout"){
            steps{
                checkout
                    $class: 'GitSCM', 
                    branches: [[name: '*/main']], // Replace 'main' with your branch name if different
                    userRemoteConfigs: [[url: 'https://github.com/your-username/your-repo.git']]
            }
          }
        }
    }
